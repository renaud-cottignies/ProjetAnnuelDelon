@{
    ViewBag.Title = "Display";
}
<script type="text/javascript">
    $('document').ready(function () {
        $('#typeOfSearch').change(function (id) {
            if($('#typeOfSearch').val()=="coloc")
            {
                $('#findColocationPref').slideDown();
                $('#findLocationPref').slideUp();
            }
            else
            {
                $('#findColocationPref').slideUp();
                $('#findLocationPref').slideDown();
            }
        });

    });
</script>


<div class="bannerTopContentGeneral">
